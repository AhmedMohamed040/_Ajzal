{
  "name": "ajzal-ecommerce",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "start:dist": "nuxt build & nuxt generate & nuxt start",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext \".js,.vue\" --fix --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "translate": "npx vue-i18n-scanner -v pages -l lang -f js -L en,ar && npx vue-i18n-scanner -v components -l lang -f js -L en,ar && npx vue-i18n-scanner -v layouts -l lang -f js -L en,ar"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/i18n": "^7.3.0",
    "apexcharts": "^3.27.3",
    "core-js": "^3.15.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nuxt": "^2.15.7",
    "vue-apexcharts": "^1.6.2",
    "vue-clickaway": "^2.2.2",
    "vue-form-wizard": "^0.8.4",
    "vue-html2pdf": "^1.8.0",
    "vue-phone-number-input": "^1.12.13",
    "vue-slick-carousel": "^1.0.6",
    "vue-svg-inline-plugin": "^2.2.0",
    "vuetify": "^2.5.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@mdi/font": "^7.2.96",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "lint-staged": "^10.5.4",
    "material-design-icons-iconfont": "^6.7.0",
    "nuxt-webfontloader": "^1.1.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-i18n-scanner": "^1.0.5"
  }
}
